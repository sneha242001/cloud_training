{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devres"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"Storage3_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Storage3'"
		},
		"ls_adf_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adf'"
		},
		"metastorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'metastorage'"
		},
		"storage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storage2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devresgroup.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devresgroup.dfs.core.windows.net/"
		},
		"Storage3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devresgroup.dfs.core.windows.net/"
		},
		"ls_adf_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devresgroup.dfs.core.windows.net/"
		},
		"metastorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devresgroup.dfs.core.windows.net/"
		},
		"storage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devresgroup.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devres_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"Storage3_accountKey": {
						"value": "[parameters('Storage3_accountKey')]"
					},
					"ls_adf_accountKey": {
						"value": "[parameters('ls_adf_accountKey')]"
					},
					"metastorage_accountKey": {
						"value": "[parameters('metastorage_accountKey')]"
					},
					"storage2_accountKey": {
						"value": "[parameters('storage2_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"Storage3_properties_typeProperties_url": {
						"value": "[parameters('Storage3_properties_typeProperties_url')]"
					},
					"ls_adf_properties_typeProperties_url": {
						"value": "[parameters('ls_adf_properties_typeProperties_url')]"
					},
					"metastorage_properties_typeProperties_url": {
						"value": "[parameters('metastorage_properties_typeProperties_url')]"
					},
					"storage2_properties_typeProperties_url": {
						"value": "[parameters('storage2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}